<!DOCTYPE html>
<html lang="en">

<%- include('partials/head.ejs', { pageName: 'Tweet Page'}) %> 

<body>
    <%- include('partials/nav.ejs') %> 
    <h3>Tweet</h3>
    <div>
        <% for(let idx=0; idx < tweets.length; idx++){ %>
            <% const tweet=tweets[idx] %>
            <%= tweet.content %>
            <%= tweet.likes %>
            <%= tweet.retweets %>
        <% } %>
    </div>

</body>

</html>